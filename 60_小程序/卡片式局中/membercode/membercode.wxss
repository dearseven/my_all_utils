.container {
  margin: 0rpx;
  padding: 0rpx;
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
  background-color: #fe9931;
}

.code_root{
 height: 100%;
  width: 100%;
}

.code_main_panel{
  width: 100%;
  height: 100%;
  background-color: #31fe12;
  display: flex;
  flex-direction: column;
  align-items: center; /*垂直居中*/
}

.code_main_panel_info{
  width: 80%;
  height: 80%;
  background-color: #fdfdfd;
  display: flex;
  flex-direction: column;
}

